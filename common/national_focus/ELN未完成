			ELN_unlock_one_propaganda_slot = yes
		}
	}
	#人事已尽，天命难违
	focus = {
		id = ELN_personnel_has_been_exhausted_destiny_of_heaven_is_inviolable
		icon = GFX_ELN_personnel_has_been_exhausted_destiny_of_heaven_is_inviolable
		x = -2
