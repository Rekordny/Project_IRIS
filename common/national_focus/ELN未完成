			focus = ELN_flames_of_war_reignit
		}
		relative_position_id = ELN_flames_of_war_reignit
		cost = 1
		allow_branch = {
		}
		ai_will_do = {
			base = 100
		}
		available = {
			surrender_progress > 0.2
			controls_state = 65
			controls_state = 52
			#NOT = { controls_state = 127 }
			any_country = {
				is_in_faction_with = ELN
				is_major = yes
				NOT = {
					is_government_in_exile = yes
				}
			}
			has_war_with = USR
			USR = { is_faction_leader = yes  controls_state = 127 }
		}
		bypass = {
		}
		available_if_capitulated = yes
		cancel_if_invalid = yes
		completion_reward = {
			#add_ideas = ELN_personnel_has_been_exhausted_destiny_of_heaven_is_inviolable_idea
			country_event = ELN_exile.1
		}
	}
	#构筑防线
	focus = {
		id = ELN_Build_a_line_of_defense
		icon = GFX_ELN_Build_a_line_of_defense
		x = 1
		y = 1
		prerequisite = {
			focus = ELN_personnel_has_been_exhausted_destiny_of_heaven_is_inviolable
		}
		relative_position_id = ELN_personnel_has_been_exhausted_destiny_of_heaven_is_inviolable
		cost = 1
		allow_branch = {
		}
		ai_will_do = {
			base = 100
		}
		available = {
			#surrender_progress > 0.25
			OR = {
				controls_state = 65
				controls_state = 52
			}
		}
		bypass = {
			NOT = {
				controls_state = 65
				controls_state = 52
			}
		}
		available_if_capitulated = yes
		cancel_if_invalid = yes
		completion_reward = {
			#add_ideas = ELN_Build_a_line_of_defense_idea
			if = {
				limit = { controls_state = 52 }
				52 = {
	                add_building_construction = {
    	                type = bunker
        	            level = 3
            	        instant_build = yes
                	    province = {
                    	    all_provinces = yes
                        	limit_to_border = yes
        	            }
            	    }
            	}
			}
			if = {
				limit = { controls_state = 65 }
				65 = {
	                add_building_construction = {
    	                type = bunker
        	            level = 5
            	        instant_build = yes
                	    province = {
                    	    all_provinces = yes
                        	limit_to_border = yes
        	            }
            	    }
					add_building_construction = {
						type = anti_air_building
						level = 3
						instant_build = yes
					}
            	}
			}
		}
	}
