			has_global_flag = January_storm_has_happened
			any_enemy_country = {
				is_major = yes
				is_IRIS_eu_country = yes
			}
		}
		bypass = {
		}
		available_if_capitulated = no
		cancel_if_invalid = yes
		completion_reward = {
