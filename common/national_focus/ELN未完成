#!gfx:interface\ELN_focus.gfx
focus_tree = {
	id = ELN_focus_tree_old
	country = {
		factor = 0
		modifier = {
			TAG = ELN
			add = 10
		}
	}
	default = no
	continuous_focus_position = {
		x = 0
		y = 900
	}


	########军备
	#重整军备
	focus = {
		id = ELN_Rearmament
		icon = GFX_ELN_Rearmament
		x = 20
		y = 0
		cost = 6
		offset = {
			x = -16
			y = 0
			trigger = {
				or = {
					has_completed_focus = ELN_victory_of_ELN_SDL_party
					has_completed_focus = ELN_victory_of_Vladilena_Milize
					has_completed_focus = ELN_victory_of_Sophie_Lefevre
				}
			}
		}
		allow_branch = {
		}
		ai_will_do = {
			base = 10
		}
		available = {
			has_global_flag = January_storm_has_happened
			has_war_support > 0.15
			not = {
				or = {
					has_idea = low_economic_mobilisation
					has_idea = partial_economic_mobilisation
					has_idea = war_economy
					has_idea = tot_economic_mobilisation
				}
			}
		}
		bypass = {
			or = {
				has_idea = low_economic_mobilisation
				has_idea = partial_economic_mobilisation
				has_idea = war_economy
				has_idea = tot_economic_mobilisation
			}
		}
		available_if_capitulated = no
		cancel_if_invalid = yes
		completion_reward = {
			if = {
				limit = {
					has_idea = civilian_economy
				}
				swap_ideas = {
					remove_idea = civilian_economy
					add_idea = low_economic_mobilisation
				}
			}
			else = {
				add_ideas = low_economic_mobilisation
			}
		}
	}
	#战争基金
	focus = {
		id = ELN_War_Fund
		icon = GFX_ELN_War_Fund
		x = 1
		y = 1
		prerequisite = {
			focus = ELN_Rearmament
		}
		relative_position_id = ELN_Rearmament
		cost = 6
		allow_branch = {
		}
		ai_will_do = {
			base = 10
		}
		available = {
		}
		bypass = {
		}
		available_if_capitulated = no
		cancel_if_invalid = yes
		completion_reward = {
			add_timed_idea = {
				idea = ELN_War_Fund_idea
				days = 200
			}
		}
	}
	#战争物资生产
	focus = {
